{
  "name": "installment-js",
  "version": "1.0.5",
  "description": "a library help to calculate installment",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "dev": "node_modules/webpack/bin/webpack.js --mode production --watch --progress --hide-modules",
    "build": "npm run clean && npm run build:installment && npm run min",
    "build:installment": "rollup lib/installment.js --name installment --format umd --file dist/installment.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "min": "npm run min:installment",
    "min:loan": "uglifyjs dist/installment.js -c -m -o dist/installment.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tranthanh98/calculate-installment.git"
  },
  "author": {
    "name": "Thanh Tran",
    "email": "trantienthanh2412@gmail.com",
    "url": "https://tranthanh98.github.io/my-cv"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tranthanh98/calculate-installment/issues"
  },
  "homepage": "https://github.com/Tranthanh98/calculate-installment#readme",
  "devDependencies": {
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "installment-js",
    "nodejs",
    "loan",
    "calculator",
    "loan-calculator",
    "js",
    "installments",
    "interest",
    "annuity",
    "annuity-due",
    "due",
    "diminishing",
    "calculate-installment"
  ]
}
